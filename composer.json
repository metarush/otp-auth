{
    "name": "metarush/otp-auth",
    "type": "library",
    "description": "Authentication library using one-time passwords (via email token)",
    "keywords": ["php", "authentication", "auth", "one-time password", "login"],
    "license": "MIT",
    "authors": [
        {
            "name": "metarush/otp-auth contributors",
            "homepage": "https://github.com/metarush/otp-auth/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "MetaRush\\OtpAuth\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "metarush/data-mapper": "^1.0",
        "phpmailer/phpmailer": "^6.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^3.1",
        "phpunit/phpunit": "^7.5"
    }
}
