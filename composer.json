{
    "name": "metarush/otp-auth",
    "type": "library",
    "description": "Authentication library using one-time passwords (via email token)",
    "keywords": ["php", "authentication", "auth", "one-time password", "login", "email token"],
    "license": "MIT",
    "authors": [
        {
            "name": "metarush/otp-auth contributors",
            "homepage": "https://github.com/metarush/otp-auth/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "MetaRush\\OtpAuth\\": "src"
        }
    },
    "require": {
        "php": ">=8.0",
        "metarush/data-access": "1.*",
        "metarush/email-fallback": "4.*",
        "laminas/laminas-diactoros": "2.*",
        "aura/session": "2.*"
    },
    "require-dev": {
        "vlucas/phpdotenv": "3.*",
        "phpunit/phpunit": "9.*",
        "phpstan/phpstan": "1.*"
    },
    "repositories": [{"type": "vcs", "url": "https://github.com/metarush/data-access"}]
}
